@model IndexViewModel

<h1>It is now @Model.PresentTime.ToShortTimeString()</h1>
<h2>Welcome all these users!</h2>
@foreach(var user in Model.Users)
{
    <p>@user</p>
}
<form action="/process" method="post">
    <div class="form-group">
        <input type="text" name="Name" id="name" placeholder="Enter a Name:">
    </div>
    <div class="form-group">
        <input type="text" name="Location" id="location" placeholder="Enter a Location:">
    </div>
    <button>Send it!</button>
</form>