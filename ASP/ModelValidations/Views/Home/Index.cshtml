@model IndexViewModel
@foreach(var user in Model.Users)
{
    <p>@user</p>
}
<form asp-action="ProcessForm" method="post">
    <div class="form-group">
        <label asp-for="NewUser.Name"></label>
        <span asp-validation-for="NewUser.Name"></span>
        <input type="text" asp-for="NewUser.Name" id="Name" placeholder="Enter a Name:">
    </div>
    <div class="form-group">
        <label asp-for="NewUser.Email"></label>
        <span asp-validation-for="NewUser.Email"></span>
        <input type="text" asp-for="NewUser.Email" id="NewUser.Email" placeholder="Enter a Email:">
    </div>
    <div class="form-group">
        <label asp-for="NewUser.DOB"></label>
        <span asp-validation-for="NewUser.DOB"></span>
        <input type="date" asp-for="NewUser.DOB" id="DOB" placeholder="Enter a DOB:">
    </div>
    <div class="form-group">
        <label asp-for="NewUser.Password"></label>
        <span asp-validation-for="NewUser.Password"></span>
        <input type="password" asp-for="NewUser.Password" id="Password" placeholder="Enter a Password:">
    </div>
    <div class="form-group">
        <label asp-for="NewUser.ComparePassword"></label>
        <span asp-validation-for="NewUser.ComparePassword"></span>
        <input type="password" asp-for="NewUser.ComparePassword" id="Password" placeholder="Repeat a Password:">
    </div>
    <button>Send it!</button>
</form>